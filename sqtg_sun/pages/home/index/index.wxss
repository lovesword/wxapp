page {
    background: #f0f0f0;
}

.indexBanner {
    width: 100%;
    height: 248rpx;
}

.header {
    padding: 0 20rpx;
    display: flex;
    position: relative;
    margin-top: -220rpx;
}

.headerLef {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    margin-right: 30rpx;
}

.haderRig {
    flex: 1;
    color: #fff;
}

.trange {
    margin: 10rpx 0;
    display: flex;
    justify-content: space-between;
}

.name,.address {
    font-size: 24rpx;
}

.trange view:nth-child(1) {
    font-size: 30rpx;
}

.trange navigator:nth-child(2) {
    padding: 0 16rpx;
    height: 38rpx;
    line-height: 38rpx;
    font-size: 24rpx;
    border: 1rpx solid #fff;
    border-radius: 8rpx;
}

.swiper {
    margin-top: 24rpx;
    padding: 0 15rpx;
    height: 300rpx;
    border-radius: 12rpx;
}

.banner-img {
    width: 100%;
    height: 300rpx;
    border-radius: 12rpx;
}

.affiche {
    height: 60rpx;
    margin: 20rpx 15rpx 0 15rpx;
    display: flex;
    align-items: center;
    background: #ffc1b1;
    border-radius: 30rpx;
}

.afficheIcon {
    width: 36rpx;
    height: 36rpx;
    margin: 0 10rpx 0 20rpx;
}

.afficheCont {
    flex: 0.96;
    overflow: hidden;
}

.afficheContTxt {
    height: 70rpx;
    line-height: 70rpx;
    color: #e9472c;
    white-space: nowrap;
    animation: 10s 0s scrolls infinite linear;
    transform: translateX(0%);
}

@keyframes scrolls {
    0% {
        transform: translateX(100%);
    }

    100% {
        transform: translateX(-100%);
    }
}

.swiper-tab {
    margin-top: 30rpx;
    height: 60rpx;
    white-space: nowrap;
}

.tab-item {
    margin-top: 7rpx;
    display: inline-block;
    border-left: 1rpx solid #c1c1c1;
    position: relative;
}

.tab-item:nth-child(1) {
    border-left: 0;
    padding-left: 0;
}

.tab-item view {
    padding: 0 35rpx;
    height: 48rpx;
    line-height: 48rpx;
    color: #999;
}

.swiper-tab .active {
    margin-top: 0;
    padding: 0 15rpx;
}

.swiper-tab .active view {
    padding: 0 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    background: #e9472c;
    color: #fff;
    border-radius: 30rpx;
}

.tab-item button {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: transparent;
}

.tab-item button::after {
    border-radius: 0;
    border: 0;
}

.goodsList {
    position: relative;
    margin: 30rpx 15rpx 0 15rpx;
    padding-bottom: 20rpx;
    background: #fff;
    border-radius: 12rpx;
}

.goodsImg {
    width: 100%;
    height: 300rpx;
    border-top-left-radius: 12rpx;
    border-top-right-radius: 12rpx;
}

.goodsName {
    margin-top: 8rpx;
    display: flex;
    align-items: center;
}

.goodsName view:nth-child(1) {
    margin-left: 20rpx;
    padding: 0 11rpx;
    height: 36rpx;
    line-height: 36rpx;
    color: #fff;
    background: #ea623e;
    font-size: 20rpx;
    border-radius: 8rpx;
}

.goodsName view:nth-child(2) {
    flex: 1;
    margin-left: 20rpx;
    padding-right: 10rpx;
    font-size: 32rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.goodsBot {
    display: flex;
    padding-left: 20rpx;
    align-items: center;
}

.gbLef {
    flex: 1;
    display: flex;
}

.txtRedbig {
    font-size: 48rpx;
    color: #e9472c;
}

.txtGray {
    margin-top: 20rpx;
    flex: 1;
    display: flex;
    white-space: nowrap;
    color: #999;
}

.gbRig {
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}

.txtGray view {
    margin-left: 15rpx;
}

.goodstime {
    position: absolute;
    right: 0;
    top: 0;
    padding: 0 25rpx;
    height: 40rpx;
    line-height: 40rpx;
    color: #fff;
    font-size: 24rpx;
    z-index: 1;
    background: #e9472c;
    border-top-right-radius: 12rpx;
}

.couponsItem {
    position: fixed;
    z-index: 12;
    padding-bottom: 30rpx;
    width: 600rpx;
    left: 50%;
    margin-left: -300rpx;
    top: 50%;
    transform: translateY(-50%);
    background: #f0f0f0;
    border-radius: 12rpx;
}

.ciClose {
    position: absolute;
    top: 30rpx;
    right: 30rpx;
    width: 30rpx;
    height: 30rpx;
}

.ciTit {
    margin: 0 30rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 30rpx;
    text-align: center;
    border-bottom: 1rpx solid #c1c1c1;
}

.ciCont {
    padding: 0 50rpx;
    box-sizing: border-box;
    height: 500rpx;
    overflow-y: scroll;
}

.ciList {
    margin-top: 30rpx;
    display: flex;
    height: 140rpx;
    border-radius: 8rpx;
    background: #fff;
}

.cilLef {
    width: 150rpx;
    padding: 0 10rpx;
    box-sizing: border-box;
    background: #e9472c;
    color: #fff;
    border-top-left-radius: 12rpx;
    border-bottom-left-radius: 12rpx;
    text-align: center;
}

.cilLef view:nth-child(1) {
    margin-top: 36rpx;
    font-size: 36rpx;
}

.cilLef view:nth-child(2) {
    font-size: 18rpx;
}

.cilRig {
    flex: 1;
    padding-left: 20rpx;
    box-sizing: border-box;
}

.cilRig view:nth-child(1) {
    margin-top: 20rpx;
    font-size: 24rpx;
}

.cilRig view:nth-child(2) {
    margin-top: 4rpx;
    font-size: 20rpx;
    color: #999;
}

.cilRig view:nth-child(3) {
    margin-top: 16rpx;
    font-size: 20rpx;
    color: #999;
}

.ciBtn {
    margin: 0 auto;
    margin-top: 40rpx;
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    background: #e9472c;
    border-radius: 40rpx;
    font-size: 30rpx;
    text-align: center;
}

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.drawer_box {
    width: 750rpx;
    position: fixed;
    bottom: 100rpx;
    left: 0;
    z-index: 1001;
    background: #fff;
    animation: showModel 0.3s ease-in-out;
}

@keyframes showModel {
    0% {
        transform: translateY(1000rpx);
    }

    100% {
        transform: translateY(0rpx);
    }
}

.drawer_title {
    display: flex;
    padding: 20rpx 20rpx;
    justify-content: space-between;
    border-bottom: 1rpx solid #ececec;
}

.cancel {
    width: 34rpx;
    height: 34rpx;
    margin-top: 20rpx;
}

.drawer_content {
    min-height: 200rpx;
    max-height: 460rpx;
    overflow-y: scroll;
    padding-bottom: 100rpx;
}

.goods-thumb {
    width: 202rpx;
    height: 202rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 8rpx;
    z-index: 999;
    box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(3,3,3,0.11);
}

.thumb {
    width: 192rpx;
    height: 192rpx;
    border-radius: 8rpx;
}

.goods-detail {
    width: 450rpx;
    margin-left: 24rpx;
    height: 166rpx;
}

.goods-title {
    font-size: 32rpx;
    line-height: 36rpx;
}

.goods-price {
    margin-top: 50rpx;
    font-size: 30rpx;
    color: #e9472c;
    font-weight: 700;
}

.goods-gg {
    font-size: 24rpx;
    color: #666;
    margin-top: 24rpx;
}

.spec {
    margin: 0rpx 20rpx;
    padding: 36rpx 0rpx 18rpx;
    border-bottom: 1rpx solid #ececec;
}

.specName {
    width: 180rpx;
}

.specValue {
    margin-top: 18rpx;
}

.specValue text {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    background: #f8f8f8;
    padding: 0rpx 35rpx;
    margin-right: 20rpx;
    margin-bottom: 10rpx;
}

.specValue .active {
    background: #e9472c;
    color: #fff;
}

.project-list {
    padding: 36rpx 20rpx 82rpx;
}

.icna {
    width: 29rpx;
    height: 30rpx;
}

.icnr {
    width: 29rpx;
    height: 3rpx;
}

.choose {
    display: flex;
    margin-top: 16rpx;
}

#chooseNum {
    display: inline-block;
    width: 62rpx;
    height: 50rpx;
    border-top: 1rpx solid #ececec;
    border-bottom: 1rpx solid #ececec;
    text-align: center;
    min-height: 0rpx;
    color: #222;
    font-weight: 700;
    font-size: 32rpx;
}

.choose view {
    width: 58rpx;
    height: 50rpx;
    border: 1rpx solid #ececec;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    font-size: 34rpx;
}

.choose view.add {
    color: #e9472c;
    border: 1rpx solid #e9472c;
}

.drawer-foot {
    width: 750rpx;
    border-top: 1rpx solid #ececec;
    height: 98rpx;
    z-index: 9999;
    background: #fff;
    position: fixed;
    bottom: 100rpx;
    left: 0;
}

.drawer-foot button {
    margin: 0 auto;
    width: 600rpx;
    height: 72rpx;
    line-height: 72rpx;
    color: #fff;
    font-size: 30rpx;
    background: #e9472c;
    margin-top: 13rpx;
    border-radius: 36rpx;
}

.drawer-foot button::after {
    border: none;
}

.iconClose {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
}

.team {
    padding: 50rpx 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.teamImg {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}

.teamCont {
    margin: 0 20rpx;
    text-align: center;
    font-size: 24rpx;
    color: #999;
}

.teamTel {
    padding: 4rpx 14rpx;
    background: #e9472c;
    border-radius: 10rpx;
    color: #fff;
}

.scrollView {
    background: #fff;
}

.coupons {
    box-sizing: border-box;
    padding: 20rpx;
}

.couponsList,.couponsLists {
    position: relative;
    display: flex;
    width: 260rpx;
    height: 140rpx;
    margin-right: 20rpx;
    background: #e9472c;
    border-radius: 8rpx;
    color: #fff;
}

.couponsList::before {
    position: absolute;
    content: '';
    left: 40rpx;
    top: -11rpx;
    width: 22rpx;
    height: 22rpx;
    background: #fff;
    border-radius: 50%;
}

.couponsList::after {
    position: absolute;
    content: '';
    left: 40rpx;
    bottom: -11rpx;
    width: 22rpx;
    height: 22rpx;
    background: #fff;
    border-radius: 50%;
}

.clTit {
    width: 30rpx;
    padding: 0 10rpx 0 10rpx;
    text-align: center;
    font-size: 24rpx;
    line-height: 26rpx;
    display: flex;
    align-items: center;
}

.clLine {
    position: relative;
    margin-top: 17rpx;
    width: 0;
    height: 110rpx;
    border-left: 1rpx dashed #fff;
}

.dotted-a {
    position: absolute;
    top: -23rpx;
    left: -11rpx;
    width: 22rpx;
    height: 22rpx;
    background: #fff;
    border-radius: 50%;
}

.dotted-b {
    position: absolute;
    bottom: -23rpx;
    left: -11rpx;
    width: 22rpx;
    height: 22rpx;
    background: #fff;
    border-radius: 50%;
}

.clCont {
    width: 176rpx;
    margin-top: 23rpx;
    padding-left: 33rpx;
}

.clBigmoney {
    font-family: Arial,Helvetica,sans-serif;
}

.clBigmoney text:nth-child(1) {
    font-size: 33rpx;
}

.clBigmoney text:nth-child(2) {
    font-size: 50rpx;
}

.clLitmoney {
    clear: both;
    float: left;
    padding: 0 10rpx;
    height: 36rpx;
    margin-top: 6rpx;
    line-height: 36rpx;
    font-size: 20rpx;
    background: #fff;
    color: #e9472c;
    border-radius: 6rpx;
    min-width: 140rpx;
}

.nlTit {
    margin-top: 30rpx;
    position: relative;
    padding-left: 43rpx;
    line-height: 88rpx;
    border-bottom: 1rpx solid #eee;
    font-size: 28rpx;
}

.nlTit:before {
    position: absolute;
    content: '';
    left: 15rpx;
    top: 30rpx;
    width: 10rpx;
    height: 28rpx;
    background: #e9472c;
    border-radius: 10rpx;
}

.couponsWrap {
    margin: 0 15rpx;
    border-radius: 15rpx;
    background: #fff;
}

.couponsnone {
    padding: 20rpx 30rpx;
    box-sizing: border-box;
}

.glMask {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 10;
    display: flex;
    justify-content: center;
    align-items: center;
    background: rgba(255,255,255,0.5);
    border-radius: 15rpx;
}

.glMask image {
    width: 127rpx;
    height: 110rpx;
}

.lineThrough {
    text-decoration: line-through;
}