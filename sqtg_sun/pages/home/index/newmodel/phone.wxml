<navigator bindtap="phone" data-phone="{{item.phone}}" hoverClass="none" style="z-index:9999999999;">
    <view class="suspend_pic" style="bottom: {{item.b_form_bottom}}%;left: calc({{item.b_form_margin}}% - 50px);background:{{item.bg_color}};opacity:{{item.form_transparent}}">
        <image src="{{item.imgurl}}"></image>
    </view>
</navigator>
