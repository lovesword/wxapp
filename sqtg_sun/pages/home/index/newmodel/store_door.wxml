<navigator class="shop_box" url="/sqtg_sun/pages/zkx/pages/storedetail/storedetail?id={{item.list[0].id}}">
    <view class="shop_logo">
        <image src="{{item.imgurl}}"></image>
    </view>
    <view class="shop_info">
        <view class="shop_name">
            <text>{{item.door_name}}</text>
        </view>
        <view class="shop_time">
            <text>营业时间：{{item.door_job}}</text>
        </view>
    </view>
    <view class="clear"></view>
    <view catchtap="phone" class="shop_phone" data-phone="{{item.door_phone}}" wx:if="{{item.door_phone}}">
        <image src="/zhy/resource/images/shop_phone_icon.png"></image>
    </view>
</navigator>
