page {
    height: 100%;
    background: #f2f2f2;
    color: #666;
}

.suggest-item {
    background: #fff;
    padding: 20rpx;
    margin-bottom: 20rpx;
}

.s-title {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #333;
    border-bottom: 1px solid #333;
}

.s-reply {
    line-height: 40rpx;
}