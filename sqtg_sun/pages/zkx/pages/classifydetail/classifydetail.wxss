.codemask { position: absolute; top: 0rpx; left: 0rpx; right: 0rpx; bottom: 0rpx; background: rgba(0, 0, 0, 0.5); z-index: 13; height: 100%; }
.code { position: absolute; top: 35%; left: 50%; transform: translate(-50%); z-index: 13; background: #fff; width: 500rpx; min-height: 250rpx; padding-bottom: 30rpx; border-radius: 10rpx; color: #ef8200; }
