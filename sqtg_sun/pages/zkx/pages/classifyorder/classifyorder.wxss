page{ background: #f2f2f2; padding-bottom: 180rpx; }
.sgoFooter { position: fixed; width: 100%; left: 0; bottom: 0; height: 100rpx; background: #fff; display: flex; align-items: center; background-color: #fff; z-index: 2; }
.sfLef { flex: 3; padding-right: 30rpx; box-sizing: border-box; text-align: right; }
.sgoBtn,.sgoBtnform{ width:220rpx; border: 0; border-radius: 0; background: #e9472c; height: 100rpx; line-height: 100rpx; color: #fff; text-align: center; font-size: 32rpx; }
.sgoBtnform{ position: absolute; top:0; right: 0; background: transparent; z-index: 1}
.sfLef view:nth-child(1){font-size: 24rpx;}
.sfLef view:nth-child(2){font-size: 30rpx;}
.sfLef view:nth-child(2) text{ color: #e9472c}
.shopname{ height: 80rpx; line-height: 80rpx; padding-left:20rpx; background-color: #fff; border-bottom: 1rpx solid #ececec; font-weight: 700; }
.right-icon{ height: 22rpx; width: 12rpx; margin-left: 20rpx; }
.pay-footer{ border-top: 1rpx solid #ececec; padding:15rpx 20rpx; }
.pay-footer>button{ width: 710rpx; height: 90rpx; line-height: 90rpx; background-color: #e9472c; font-size: 34rpx; color: #fff; border-radius: 16rpx; }
.choicePay{ animation: showModel 0.3s ease-in-out; }
@keyframes showModel{ 0%{ transform: translateY(1000rpx) }
100%{ transform: translateY(0rpx) }
}.choose-type{ padding:20rpx 20rpx; margin-top: 20rpx; background-color: #fff; }
.shop-addr{ display: inline-block; width: 500rpx; text-align: right; }
.nlTit{ margin-top:20rpx; position: relative; display: flex; align-items: center; padding:0 15rpx 0 30rpx; background: #fff; border-bottom: 1rpx solid #eee;}
.nlTit view:nth-child(1){line-height: 88rpx;}
.nlTit view:nth-child(2){ padding:20rpx 0; flex: 1; font-size: 28rpx; }
.linkaddress{ padding: 30rpx 0; background: #fff; font-size: 28rpx;}
.laTop{ padding: 0 15rpx 0 30rpx; display: flex; align-items: center;}
.laTop image{ margin-left:6rpx; width: 42rpx; height: 42rpx; border-radius: 50%;}
.laTop view:nth-child(2){ margin-left:20rpx; font-size: 30rpx;}
.laTop view:nth-child(3){ margin-left: 20rpx; font-size: 24rpx}
.laBot{ display: flex; align-items:center; padding:15rpx 15rpx 0 30rpx;}
.laBot image{ margin-left:10rpx; width:36rpx; height: 36rpx;}
.goods{ display: flex; padding: 20rpx 15rpx; background: #fff; border-bottom: 1rpx solid #eee;}
.gLef{ width: 180rpx; height: 180rpx; margin-right: 15rpx;}
.gRig{ flex: 1; box-sizing: border-box;}
.grTop{ display: flex; font-size: 24rpx;}
.grTop view:nth-child(1),.grCont view:nth-child(1){ flex: 1}
.grCont{ margin-top:10rpx; display: flex; color: #999; font-size: 24rpx;}
.grBot{ margin-top:70rpx; display: flex; justify-content: flex-end; align-items: center;}
.grBot view:nth-child(1){font-size: 24rpx;}
.grBot view:nth-child(2){font-size: 30rpx; color: #e9472c;}
.coupons{ padding:0 15rpx; height: 88rpx; display: flex; align-items: center; background: #fff;}
.coupons view:nth-child(1){ font-size: 28rpx; flex: 1}
.coupons view:nth-child(2){ font-size: 24rpx; color: #e9472c;}
.coupons image:nth-child(3){ margin:0 15rpx 0 10rpx; width: 12rpx; height: 24rpx;}
.couponsItem{ position: fixed; z-index: 12; padding-bottom: 50rpx; width: 600rpx; left: 50%; margin-left:-300rpx; top: 50%; transform: translateY(-50%); background: #f0f0f0; border-radius: 12rpx;}
.ciClose{ position: absolute; top:30rpx; right: 30rpx; width: 30rpx; height: 30rpx;}
.ciTit{ margin:0 30rpx; height:88rpx; line-height: 88rpx; font-size: 30rpx; text-align: center; border-bottom:1rpx solid #c1c1c1 }
.ciCont{ padding: 0 50rpx; box-sizing: border-box; height: 500rpx; overflow-y: scroll;}
.ciList{ position: relative; margin-top:30rpx; display: flex; height: 140rpx; border-radius: 8rpx; background: #fff; box-shadow: 0 0 10rpx rgba(0, 0, 0,0.1)}
.cilLef,.cilLefgray{ width:150rpx; padding:0 10rpx; box-sizing: border-box; background: #e9472c; color: #fff; border-top-left-radius: 12rpx; border-bottom-left-radius: 12rpx; text-align: center}
.cilLefgray{background:#999}
.cilLef view:nth-child(1),.cilLefgray view:nth-child(1){ margin-top:36rpx; font-size: 36rpx;}
.cilLef view:nth-child(2),.cilLefgray view:nth-child(2){ font-size: 18rpx;}
.cilRig{ flex: 1; padding-left:20rpx; box-sizing: border-box;}
.cilRig view:nth-child(1){ margin-top:20rpx; font-size:24rpx;}
.cilRig view:nth-child(2){ margin-top:4rpx; font-size:20rpx; color: #999;}
.cilRig view:nth-child(3){ margin-top:16rpx; font-size:20rpx; color: #999;}
.ciBtn{ margin:0 auto; margin-top:40rpx; width: 300rpx; height: 80rpx; line-height: 80rpx; color: #fff; background: #e9472c; border-radius: 40rpx;font-size: 30rpx; text-align: center;}
.ciList::before{position: absolute; content: ''; left: 135rpx; top: -15rpx; width: 30rpx; height: 30rpx; background: #f2f2f2; border-radius: 50%;}
.ciList::after{position: absolute; content: ''; left: 135rpx; bottom: -15rpx; width: 30rpx; height: 30rpx; background: #f2f2f2; border-radius: 50%;}
.footerCoupons{ width: 100%; position: fixed; left: 0; bottom:100rpx; height: 64rpx; line-height: 64rpx; padding-right: 15rpx; background: #ffc1b1; color: #e9472c; font-size: 28rpx; text-align: right;box-sizing:border-box; }
.isMaskPhone {width: 100%;height: 100%;position: fixed;top: 0;left: 0;z-index: 1000;background: #000;opacity: 0.5;}
#isPhone {position: fixed;width: 480rpx;height: 250rpx;background: #fff;top: 50%;left: 50%;margin-left: -265rpx;margin-top: -185rpx;z-index: 1001;padding: 50rpx 50rpx;border-radius: 12rpx;}
.isLoginBtn {display: flex;justify-content: space-around;}
.isLoginBtn button,.isLoginBtn wx-navigator {background: #fff; height: 68rpx; line-height: 68rpx; font-size: 28rpx; width: 260rpx; text-align: center;color: #4bad37;border:1rpx solid #eee; border-radius: 12rpx; }
.isLoginBtn wx-navigator{ margin-right: 20rpx; background: #f2f2f2; color: #999;}
.loginTitle {font-size: 34rpx;}
.shopImg {height: 80rpx;display: flex;align-items: center;padding: 20rpx 0rpx;justify-content: center; font-size: 28rpx;}
.shopImg>image {margin:0 14rpx 0 0;width: 68rpx;height: 68rpx;border-radius: 50%;}
.colorLine{ display: block; height: 10rpx; width: 100%; margin-bottom: 40rpx;}
.fw600{font-weight: 600}
