page{ background: #f2f2f2; }
.swiper-tab{ position: relative; }
.shouldFiexd{ position: fixed; top:0rpx; left: 0rpx; z-index: 10; animation: headFix 0.3s 1 }
@keyframes headFix{ 0%{ top:-100rpx; }
100%{ top:0; }
}.session{ padding-bottom: 30rpx; }
.couponsItem { display: flex; margin: 0 30rpx; margin-top: 30rpx; }
.couponsLef { width: 200rpx; height: 150rpx; background: #e9472c; text-align: center; color: #fff; }
.couponsLefPass { background-color: #666; }
.couponsRig { flex: 1; background: #ffdbd6; padding-left: 30rpx; overflow: hidden; }
.couponsRigPass { background-color: #ccc; }
.couponsSale { margin-top: 30rpx; font-size: 42rpx; }
.couponsSale text, .couponsSatisfy, .couponsTime, .couponsLimit { font-size: 24rpx; }
.couponsTit { margin-top: 20rpx; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; }
.couponsTime, .couponsLimit { margin-top: 6rpx; }
.couponsLimit { width: 90%; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; }
.swiper-tab { width: 100%; display: flex; height: 89rpx; line-height: 89rpx; text-align: center; border-bottom: 1px solid #eee; justify-content: space-between; background: #fff; font-size:28rpx; }
.swiper-tab .active { height: 90rpx; line-height: 89rpx; border-bottom: 2rpx solid #e9472c; color: #e9472c; font-size: 36rpx }
.swiper-tab view:nth-child(1) { margin-left: 30rpx; }
.swiper-tab view:last-child { margin-right: 30rpx; }
.ciList{ position: relative; margin:30rpx 15rpx 0 15rpx; height: 150rpx; display: flex; border-radius: 8rpx; background: #fff; box-shadow: 0 0 10rpx rgba(0, 0, 0,0.1)}
.cilLef{ width:150rpx; padding:0 10rpx; box-sizing: border-box; background: #e9472c; color: #fff; border-top-left-radius: 12rpx; border-bottom-left-radius: 12rpx; text-align: center}
.cilLef view:nth-child(1){ margin-top:41rpx; font-size: 36rpx;}
.cilLef view:nth-child(2){ font-size: 18rpx;}
.cilRig{ position: relative; flex: 1; padding-left:20rpx; box-sizing: border-box;}
.cilRig view:nth-child(1){ margin-top:20rpx; font-size:24rpx;}
.cilRig view:nth-child(2){ margin-top:8rpx; font-size:20rpx; color: #999;}
.cilRig view:nth-child(3){ margin-top:20rpx; font-size:20rpx; color: #999;}
.ciBtn{ margin:0 auto; margin-top:40rpx; width: 300rpx; height: 80rpx; line-height: 80rpx; color: #fff; background: #e9472c; border-radius: 40rpx;font-size: 30rpx; text-align: center;}
.ciList::before{position: absolute; content: ''; left: 135rpx; top: -15rpx; width: 30rpx; height: 30rpx; background: #f2f2f2; border-radius: 50%;}
.ciList::after{position: absolute; content: ''; left: 135rpx; bottom: -15rpx; width: 30rpx; height: 30rpx; background: #f2f2f2; border-radius: 50%;}
.cilRig image{ position: absolute; top:40rpx; right: 30rpx; width: 94rpx; height: 80rpx;}
.outdate{background: #999}
