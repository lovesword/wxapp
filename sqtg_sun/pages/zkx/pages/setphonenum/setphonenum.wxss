page{background: #f2f2f2; height: 100%}
.wrap{ margin:4% 0 0 4%; width: 92%; height: 92%; background: #fff; border-radius: 20rpx;}
.inpWrap{ padding-top:50rpx; display: flex; height: 80rpx; align-items: center;}
.inpWrap label{ width: 220rpx; font-size: 26rpx; text-align: right}
.inpWrap input{ margin:0 30rpx 0 15rpx; padding-left:16rpx; border:1rpx solid #ddd; height: 80rpx; line-height: 80rpx; flex: 1; font-size: 30rpx; border-radius: 12rpx;}
.formBtn{ width: 300rpx; height: 70rpx; line-height: 70rpx; margin:0 auto; margin-top:60rpx; background: #fff; background: #e9472c; color: #fff; font-size: 30rpx;}
